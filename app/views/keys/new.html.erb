<h1>New Key</h1>

<%= form_with (model: @key, url: [@property, @key] )  do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :code %>
    <%= form.text_field :code %>
  </div>

  <div>
    <%= form.submit "Create Key" %>
  </div>
<% end %>

<%= link_to "Back", keys_path %>
