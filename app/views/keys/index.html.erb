<h1>Keys</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
    </tr>
  </thead>
  <tbody>
    <% @keys.each do |key| %>
      <tr>
        <td><%= key.name %></td>
        <td><%= key.code %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Key", new_key_path %>
